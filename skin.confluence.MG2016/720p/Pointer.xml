<?xml version="1.0" encoding="UTF-8"?>
<window>
	<defaultcontrol>-</defaultcontrol>
	<coordinates>
		<system>1</system>
		<left>0</left>
		<top>0</top>
	</coordinates>
	<controls>
		<control type="image" id="1">
			<description>Pointer Image</description>
			<left>0</left>
			<top>0</top>
			<!--width>64</width>
			<height>64</height-->
			<texture fallback="pointer-nofocus.png">$INFO[Skin.String(CustomPointerPathNF)]</texture>
			<!--texture>wand128.png</texture-->
			<!--texture>pointer-nofocus.png</texture-->
			<!--colordiffuse>0xCCffffff</colordiffuse-->
			<include condition="IsEmpty(Skin.String(CustomPointerSize))">Size64x64</include>
			<include condition="StringCompare(Skin.String(CustomPointerSize),16)">Size16x16</include>
			<include condition="StringCompare(Skin.String(CustomPointerSize),24)">Size24x24</include>
			<include condition="StringCompare(Skin.String(CustomPointerSize),32)">Size32x32</include>
			<include condition="StringCompare(Skin.String(CustomPointerSize),48)">Size48x48</include>
			<include condition="StringCompare(Skin.String(CustomPointerSize),64)">Size64x64</include>
			<include condition="StringCompare(Skin.String(CustomPointerSize),90)">Size90x90</include>
			<include condition="StringCompare(Skin.String(CustomPointerSize),120)">Size120x120</include>
			<include condition="StringCompare(Skin.String(CustomPointerSize),128)">Size128x128</include>
			<include condition="StringCompare(Skin.String(CustomPointerSize),218)">Size218x218</include>
			<include condition="StringCompare(Skin.String(CustomPointerSize),256)">Size256x256</include>
		</control>
